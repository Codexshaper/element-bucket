{
  "name": "codexshaper/element-bucket",
  "description": "A premium elements bucket for elementor.",
  "type": "library",
  "license": "gpl-2.0",
  "version": "1.0.0",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://github.com/Codexshaper/element-bucket/",
  "autoload": {
    "psr-4": {
      "CodexShaper\\ElementBucket\\": [
        "src/",
        "dev/"
      ]
    }
  },
  "authors": [
    {
      "name": "Md Abu Ahsan Basir",
      "email": "maab.career@gmil.com"
    },
    {
      "name": "CodexShaper",
      "email": "codexshaper@gmil.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "3.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
